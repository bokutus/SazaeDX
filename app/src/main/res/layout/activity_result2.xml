<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ResultActivity2">

    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/resultLabel"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toTopOf="@+id/myHandImage"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/comHandImage"/>
    <ImageView
            android:layout_width="397dp"
            android:layout_height="230dp" app:srcCompat="@drawable/sazae_gu"
            android:id="@+id/comHandImage"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:scaleType="fitCenter" android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.284" app:layout_constraintHorizontal_bias="0.0"/>
    <ImageView
            android:layout_width="153dp"
            android:layout_height="171dp" app:srcCompat="@drawable/gu"
            android:id="@+id/myHandImage" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="48dp" app:layout_constraintBottom_toBottomOf="parent"
            android:scaleType="fitCenter" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"/>
    <Button
            android:text="戻る"
            android:layout_width="109dp"
            android:layout_height="65dp"
            android:id="@+id/backButton" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/comHandImage" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.027"
            app:layout_constraintVertical_bias="0.307"/>
</androidx.constraintlayout.widget.ConstraintLayout>